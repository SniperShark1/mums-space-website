{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/dist/index.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/dist/public/index.html" }
  ],
  "outputDirectory": "dist/public",
  "buildCommand": "npm run build",
  "installCommand": "npm install --production=false"
}
